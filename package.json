{
  "name": "factorio-blueprint-upgrade-patcher",
  "description": "Plan your upgrades in config, Patch your blueprint by uploading in site, Profit!",
  "version": "0.2.0",
  "engines": {
    "node": ">=15.0.0"
  },
  "private": true,
  "scripts": {
    "build": "qwik build",
    "build.client": "vite build",
    "build.preview": "vite build --ssr src/entry.preview.tsx",
    "build.server": "vite build -c adapters/cloudflare-pages/vite.config.ts",
    "build.types": "tsc --incremental --noEmit",
    "deploy": "wrangler pages publish ./dist",
    "dev": "vite --mode ssr",
    "dev.debug": "node --inspect-brk ./node_modules/vite/bin/vite.js --mode ssr --force",
    "fmt": "prettier --write .",
    "fmt.check": "prettier --check .",
    "lint": "eslint --fix \"src/**/*.ts*\"",
    "preview": "qwik build preview && vite preview --open",
    "serve": "wrangler pages dev ./dist",
    "start": "vite --open --mode ssr",
    "qwik": "qwik",
    "mega-lint": "npx mega-linter-runner"
  },
  "devDependencies": {
    "@builder.io/qwik": "^1.2.6",
    "@builder.io/qwik-city": "^1.2.6",
    "@types/eslint": "^8.44.0",
    "@types/file-saver": "^2.0.5",
    "@types/node": "^20.4.2",
    "@types/pako": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "eslint": "8.44.0",
    "eslint-plugin-qwik": "^1.2.6",
    "prettier": "^3.0.0",
    "prettier-config-mrdgh2821": "^3.0.0",
    "prettier-plugin-organize-imports": "^3.2.2",
    "prettier-plugin-packagejson": "^2.4.5",
    "stylelint": "^15.10.1",
    "typescript": "5.1.6",
    "undici": "5.22.1",
    "vite": "^4.4.3",
    "vite-tsconfig-paths": "4.2.0",
    "wrangler": "^3.2.0"
  },
  "dependencies": {
    "byte-base64": "^1.1.0",
    "file-saver": "^2.0.5",
    "pako": "^2.1.0"
  }
}
